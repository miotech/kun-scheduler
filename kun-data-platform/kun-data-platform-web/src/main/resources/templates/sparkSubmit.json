{
  "name": "SparkSubmit",
  "templateType": "spark",
  "templateGroup": "development",
  "operator": {
    "name": "sparkSubmit-operator",
    "description": "Spark Submit Operator",
    "className": "com.miotech.kun.workflow.operator.SparkSubmitOperator"
  },
  "displayParameters": [
    {
      "name": "class",
      "type": "string",
      "required": true,
      "displayName": "class"
    },
    {
      "name": "application",
      "type": "string",
      "required": true,
      "displayName": "application jar/file"
    },
    {
      "name": "args",
      "type": "text",
      "required": false,
      "displayName": "application args"
    },
    {
      "name": "sparkConf",
      "type": "keyvalue",
      "required": false,
      "displayName": "spark configuration"
    }
  ],
  "defaultValues": {
    "yarnHost": "${ dataplatform.yarn.host }",
    "sparkConf": "{}",
    "variables": "{}"
  },
  "renderClassName": "com.miotech.kun.dataplatform.common.tasktemplate.renderer.SparkSubmitBasedTaskTemplateRender"
}