{
    "name": "Google Sheet",
    "templateType": "spark",
    "templateGroup": "dataIntegration",
    "operator": {
      "name": "spark-operator",
      "description": "Spark Operator",
      "className": "com.miotech.kun.workflow.operator.SparkOperator"
    },
    "displayParameters": [
      {
        "name": "tableName",
        "type": "string",
        "required": true,
        "displayName": "Table Name"
      },
      {
        "name": "sheetUrl",
        "type": "string",
        "required": true,
        "displayName": "Google Sheet Url"
      },
      {
        "name": "readOrWriteMode",
        "type": "string",
        "required": true,
        "displayName": "Read or Write mode"
      },
      {
        "name": "insertMode",
        "type": "string",
        "required": false,
        "displayName": "Write mode"
      },
      {
        "name": "emptyAsNull",
        "type": "string",
        "required": false,
        "displayName": "Empty as Null"
      },
      {
        "name": "serviceAccount",
        "type": "string",
        "required": false,
        "displayName": "Service Account"
      },
      {
        "name": "credentialPath",
        "type": "string",
        "required": false,
        "displayName": "Credential Path"
      }
    ],
    "defaultValues": {
      "args": "-e dev -m TASK_CENTER -s GENERAL_GOOGLE_SHEET_STEP",
      "jars": "${ spark.listener.jar }",
      "files": "${ dataplatform.etl.jar }",
      "livyHost": "${ dataplatform.livy.host }",
      "sparkConf": "{\"kun.dataplatform.insertMode\":\"overwrite\",\"kun.dataplatform.emptyAsNull\":\"true\",\"kun.dataplatform.credentialPath\":\"s3://com.miotech.data.prd/google/credentials/google_service_credential.p12\",\"kun.dataplatform.serviceAccount\":\"jentle@bubbly-mission-267911.iam.gserviceaccount.com\",\"kun.dataplatform.readOrWriteMode\":\"READ\",\"spline.hdfs_dispatcher.address\":\"${ spark.hdfs.address }\"}",
      "variables": "{}",
      "application": "com.miotech.etl.knowledge_graph.app.Application"
    },
    "renderClassName": "com.miotech.kun.dataplatform.common.tasktemplate.renderer.SparkConfigTaskTemplateRender"
}
