{
  "name": "SparkSubmitSQL",
  "templateType": "sql",
  "templateGroup": "development",
  "operator": {
    "name": "sparkSubmitSql-operator",
    "description": "Spark Submit Operator",
    "className": "com.miotech.kun.workflow.operator.SparkSubmitSqlOperator"
  },
  "defaultValues": {
    "yarnHost": "${ dataplatform.yarn.host }",
    "class": "com.miotech.kun.sql.Application",
    "application": "${ dataplatform.spark.sql.jar }",
    "variables": "{}",
    "sparkConf": "{}"
  },
  "displayParameters": [
    {
      "name": "master",
      "type": "string",
      "required": false,
      "displayName": "master"
    },
    {
      "name": "proxy-user",
      "type": "string",
      "required": false,
      "displayName": "proxy-user"
    },
    {
    "name": "args",
    "displayName": "sql",
    "type": "sql",
    "required" : true
    },
    {
    "name": "sparkConf",
    "type": "keyvalue",
    "required": false,
    "displayName": "Extra Spark Configuration"
    }
  ],
  "renderClassName": "com.miotech.kun.dataplatform.common.tasktemplate.renderer.SparkSubmitBasedTaskTemplateRender"
}