{
  "name": "General ETL Step",
  "templateType": "spark",
  "templateGroup": "development",
  "operator": {
    "name": "spark-operator",
    "description": "Spark Operator",
    "className": "com.miotech.kun.workflow.operator.SparkOperator"
  },
  "displayParameters": [
    {
      "name": "environment",
      "type": "string",
      "required": true,
      "displayName": "Environment"
    },
    {
      "name": "etlClassName",
      "type": "string",
      "required": true,
      "displayName": "ETL ClassName"
    },
    {
      "name": "sparkConf",
      "type": "keyvalue",
      "required": false,
      "displayName": "Extra Spark Configuration"
    }
  ],
  "defaultValues": {
    "args": "-e dev -m TASK_CENTER -s GENERAL_ETL_STEP",
    "jars": "${ spark.listener.jar }",
    "files": "${ dataplatform.etl.jar }",
    "livyHost": "${ dataplatform.livy.host }",
    "sparkConf": "{\"kun.dataplatform.environment\": \"dev\",\"spark.hadoop.spline.hdfs_dispatcher.address\":\"${ spark.hdfs.address }\"}",
    "variables": "{}",
    "application": "com.miotech.etl.knowledge_graph.app.Application"
  },
  "renderClassName": "com.miotech.kun.dataplatform.common.tasktemplate.renderer.SparkConfigTaskTemplateRender"
}