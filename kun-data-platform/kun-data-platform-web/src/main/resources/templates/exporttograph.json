{
  "name": "Export to Graph",
  "templateType": "spark",
  "templateGroup": "dataIntegration",
  "operator": {
    "name": "spark-operator",
    "description": "Spark Operator",
    "className": "com.miotech.kun.workflow.operator.SparkOperator"
  },
  "displayParameters": [
    {
      "name": "tableName",
      "type": "string",
      "required": true,
      "displayName": "table name"
    },
    {
      "name": "dbName",
      "type": "string",
      "required": true,
      "displayName": "Arango Database Name"
    },
    {
      "name": "collectionName",
      "type": "string",
      "required": true,
      "displayName": "Arango Collection Name"
    },
    {
      "name": "graphDataType",
      "type": "string",
      "required": true,
      "displayName": "Graph Data Type: edge/node"
    },
    {
      "name": "operationType",
      "type": "string",
      "required": true,
      "displayName": "Operation Type"
    },
    {
      "name": "dropFirst",
      "type": "string",
      "required": true,
      "displayName": "Drop before insert"
    },
    {
      "name": "partition",
      "type": "string",
      "required": true,
      "displayName": "Partition Number"
    },
    {
      "name": "environment",
      "type": "string",
      "required": true,
      "displayName": "Envrionment"
    },
    {
      "name": "sparkConf",
      "type": "keyvalue",
      "required": false,
      "displayName": "spark configuration"
    }
  ],
  "defaultValues": {
    "args": "--sparkConf arangodb.connectionTtl=900000 -e dev -m TASK_CENTER -s GENERAL_GRAPH_STEP",
    "jars": "",
    "files": "${ dataplatform.etl.jar }",
    "livyHost": "${ dataplatform.livy.host }",
    "sparkConf": "{\"kun.dataplatform.operationType\":\"insert\", \"spark.dynamicAllocation.maxExecutors\":\"15\"}",
    "variables": "{}",
    "application": "com.miotech.etl.knowledge_graph.app.Application"
  },
  "renderClassName": "com.miotech.kun.dataplatform.common.tasktemplate.renderer.SparkConfigTaskTemplateRender"
}