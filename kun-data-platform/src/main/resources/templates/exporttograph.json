  "templateType": "spark",
  "templateGroup": "dataIntegration",
  "operator": {
    "name": "Spark_Operator",
    "description": "Spark Operator",
    "className": "com.miotech.kun.workflow.operator.SparkOperator"
  },
  "displayParameters": [
    {
      "name": "tableName",
      "type": "string",
      "required": true,
      "displayName": "table name"
    },
    {
      "name": "dbName",
      "type": "string",
      "required": true,
      "displayName": "Arango Database Name"
    },
    {
      "name": "collectionName",
      "type": "string",
      "required": true,
      "displayName": "Arango Collection Name"
    },
    {
      "name": "graphDataType",
      "type": "string",
      "required": true,
      "displayName": "Graph Data Type: edge/node"
