    {
      "name": "operationType",
      "type": "string",
      "required": true,
      "displayName": "Operation Type"
    },
    {
      "name": "dropFirst",
      "type": "string",
      "required": true,
      "displayName": "Drop before insert"
    },
    {
      "name": "partition",
      "type": "string",
      "required": true,
      "displayName": "Partition Number"
    },
    {
      "name": "environment",
      "type": "string",
      "required": true,
      "displayName": "Envrionment"
