#####################
# User Config
#####################

# Timezone
TZ=Asia/Shanghai

# Version
KUN_VERSION=0.7.0-rc1

# Port
LISTEN_PORT=8080

#####################
# System Config
#####################

IMAGE_REMOTE_ORG=miotechoss/

POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres
POSTGRES_DB=kun

DB_PG_IP=kun-postgres
DB_PG_PORT=5432
